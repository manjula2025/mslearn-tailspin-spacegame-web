{
  "name": "DotNet8DevContainer",
  "dockerFile": "Dockerfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-vscode.azurecli",
        "ms-vscode.powershell",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "remoteUser": "vscode",
  "build": {
    "args": {
      "NODE_VERSION": "16"
    }
  },
  "postStartCommand": "dotnet --info"
}
